<%= image_tag("images.jpeg", :alt => "test image" ,:style =>"width:200px;")%>

<%= search_form_for @search, url: employee_index_path  do |f| %>
<%= f.label :name_cont, "username Contain" %>
<%= f.text_field :name_cont %>
<%= f.submit 'search' %>
<%end %>

<h1> welcome to your account <h1>
	<%= link_to 'home',employee_index_path %>
	<%= link_to 'AddRole', add_role_user_index_path %>
	<%= link_to 'show',user_path(session['user_id']) %>
	<%= link_to 'logout', logout_session_index_path %><br></br>
	<%= paginate @user, :window => 3 %>
	<% @user.each do |f| %>
		<% if f.user.role.role_name != 'admin' %>
			<%= link_to f.name, user_path(f) %>
		<% end %>
	<% end %><br /><br />
	<%= link_to 'editphoto', edit_photo_user_index_path %>
